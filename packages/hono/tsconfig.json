{
    "compilerOptions": {
        "target": "ES2022",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "lib": ["es2022"],
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node"
    }
}